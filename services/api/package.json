{
  "name": "@kagchi/qristy-api",
  "version": "1.0.0",
  "description": "QRISty Backend API.",
  "main": "dist",
  "scripts": {
    "dev": "pnpm run build && node -r dotenv/config dist/index.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "rimraf dist && swc src -d dist --config-file .swcrc --strip-leading-paths"
  },
  "type": "module",
  "author": "KagChi",
  "license": "GPL-3.0",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    },
    "extends": [
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {}
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^12.0.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.4",
    "@types/node": "^20.14.13",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.23.1",
    "eslint": "^8.57.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "drizzle-orm": "^0.32.1",
    "got": "^14.4.2",
    "hono": "^4.5.3",
    "hono-pino-logger": "^1.0.0",
    "mysql2": "^3.11.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "socket.io": "^4.7.5"
  }
}
